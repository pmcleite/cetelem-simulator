{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ]
    },
    "jsRules": {}
}
